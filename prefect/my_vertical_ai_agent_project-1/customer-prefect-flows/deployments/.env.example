# Environment configuration for Prefect 3.x Customer Flows

# Prefect Configuration
PREFECT_API_URL=http://localhost:4200/api
PREFECT_API_KEY=your_prefect_api_key_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# LangChain Configuration
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=customer-flows
LANGCHAIN_TRACING_V2=false

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO

# Database Configuration (if using PostgreSQL)
POSTGRES_DB=prefect
POSTGRES_USER=prefect
POSTGRES_PASSWORD=prefect

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379

# Storage Configuration
DATABASE_URL=postgresql://prefect:prefect@localhost:5432/prefect

# Worker Configuration
PREFECT_WORKER_POOL=default
PREFECT_WORKER_CONCURRENCY=4
